{
    "title": "Space as Fn if long pressed",
    "rules": [{
        "description": "Space as Fn if long pressed",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "spacebar",
                "modifiers": {
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [{
                "key_code": "fn"
            }],
            "to_if_alone": [{
                "key_code": "spacebar"
            }]
        }]
    }]
}
