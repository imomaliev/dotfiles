{
    "title": "Fn Arrow Keys",
    "rules": [{
        "description": "Arrow Keys (fn + hynu to arrow keys)",
        "manipulators": [{
                "type": "basic",
                "from": {
                    "key_code": "n",
                    "modifiers": {
                        "mandatory": [
                            "fn"
                        ]
                    }
                },
                "to": [{
                    "key_code": "up_arrow"
                }]
            },
            {
                "type": "basic",
                "from": {
                    "key_code": "h",
                    "modifiers": {
                        "mandatory": [
                            "fn"
                        ]
                    }
                },
                "to": [{
                    "key_code": "left_arrow"
                }]
            },
            {
                "type": "basic",
                "from": {
                    "key_code": "y",
                    "modifiers": {
                        "mandatory": [
                            "fn"
                        ]
                    }
                },
                "to": [{
                    "key_code": "down_arrow"
                }]
            },
            {
                "type": "basic",
                "from": {
                    "key_code": "u",
                    "modifiers": {
                        "mandatory": [
                            "fn"
                        ]
                    }
                },
                "to": [{
                    "key_code": "right_arrow"
                }]
            }
        ]
    }]
}
