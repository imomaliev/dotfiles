{
    "title": "Tmux and vim",
    "rules": [
        {
            "description": "Tmux prifix key Ctrl+B",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f20"
                    },
                    "to": [
                        {
                            "key_code": "b",
                            "modifiers": [
                                "left_control"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "Vim Ctrl+^",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f20",
                        "modifiers": {
                            "mandatory": [
                                "left_shift"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "6",
                            "modifiers": [
                                "left_control",
                                "left_shift"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
